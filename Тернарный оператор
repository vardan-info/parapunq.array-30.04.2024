
public class Main
{
	public static void main(String[] args) {
	 int money =100;
	 int brdeyPrice=20;
	 int moneyWorker=15;
	 int exchange =money-brdeyPrice;
	 int childMoney=exchange>=50 ? 10 :5;
	 System.out.println(childMoney);
	 
	 
	 if(moneyWorker>=11){
	     exchange=20;
	     moneyWorker-=10;
	 }else{
	     exchange=5;
	     moneyWorker -=11;
	 }
	 
	}
}

result......-->10
