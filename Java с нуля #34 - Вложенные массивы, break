public class Main {
    public static void main(String[] args) {

int[]numbers1={1,2,3,4,5};
        int[]numbers2={1,2,3,4,5};
        int[][] multyArray={numbers1,numbers2};
        for(int[]array:multyArray){
            for(int number:array){
                if(number==5){
                    System.out.println("number");
                }

            }
        }
  }
    }
